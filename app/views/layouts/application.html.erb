<!DOCTYPE html>
<html>
<head>
  <title>Bucketlust</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<nav>
		<ul>
			<% if user_signed_in? %>
				<li><%= link_to 'Home', root_path %></li>
				<li><%= link_to 'Sign Out', destroy_user_session_path, method: :delete %></li>
			<% else %>
				<li><%= link_to 'Sign Up', new_user_registration_path %></li>
				<li><%= link_to 'Sign In', new_user_session_path %></li>
			<% end %>
		</ul>
	</nav>
	<p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
  
<%= yield %>
	<script src="http://cdnjs.cloudflare.com/ajax/libs/gmaps.js/0.4.23/gmaps.min.js"></script>
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBjNYCVQWZtDiDEoupDaJF25qSVHETk0zU" ></script>  
</body>
</html>
